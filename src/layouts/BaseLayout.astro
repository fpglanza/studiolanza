---
import '../styles/global.css';
import '../styles/lightbox.css';
const { title = "studiolanza" } = Astro.props;
---
<!doctype html>
<html lang="it">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
  </head>
  <body class="bg-zinc-50 text-zinc-900 antialiased">
    <slot />
  </body>
</html>
